{
  "name": "primitive",
  "author": "Tania Rascia",
  "version": "1.3.0",
  "description": "A Sass boilerplate for front end web developers.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taniarascia/primitive.git"
  },
  "keywords": [
    "sass",
    "setup",
    "boilerplate",
    "framework",
    "ui",
    "css framework"
  ],
  "dependencies": {
    "gulp": "^3.9.1",
    "node-sass": "*"
  },
  "scripts": {
    "sass-docs": "node-sass -w -r ./src/scss/ -o ./docs/css/",
    "sass": "node-sass ./src/scss/ -o ./dist/css/  -w -r --source-map-embed --source-map-contents",
    "autoprefixer": "postcss -u autoprefixer -r ./dist/css/*"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taniarascia/primitive/issues"
  },
  "homepage": "https://taniarascia.github.io/primitive",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^9.0.0",
    "postcss-cli": "^5.0.1"
  }
}
